
{
  "name": "Sort Your Tabs",
  "description": "Chrome浏览器Tab开启太多了，怎么办法？该拓展程序会帮助您记录访问Tab的时间，并可以按照最近最少使用算法（LRU）对您的Tabs进行排序，您就可以快速找到您最近使用的Tab",
  "version": "1.0.2",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "images/2SortYourTabs16.png",
    "default_title": "Press Ctrl(Win)/Command(Mac)+Shift+Right to Sort Your Tabs by LRU"
  },
  "commands": {
    "data-init": {
      "suggested_key": {
        "default": "Ctrl+Shift+1",
        "mac": "Command+Shift+1"
      },
      "description": "init"
    },
    "tab-recover": {
      "suggested_key": {
        "default": "Ctrl+Shift+0",
        "mac": "Command+Shift+0"
      },
      "description": "recover"
    },
    "lru-tabs-right": {
      "suggested_key": {
        "default": "Ctrl+Shift+Right",
        "mac": "Command+Shift+Right"
      },
      "description": "Sort Your Tabs form Left to Right"
    }
  },
  "icons": {
    "16": "images/2SortYourTabs16.png",
    "32": "images/2SortYourTabs32.png",
    "48": "images/2SortYourTabs48.png",
    "128": "images/2SortYourTabs128.png"
  }
}